[
  {
    "kernelSource": {
      "name": "gtanotexllteStock",
      "repo": "https://github.com/Marco-cefetmg/android_kernel_samsung_exynos7870",
      "branch": "master",
      "device": "gtanotexllte",
      "defconfig": "exynos7870-gtanotexllte_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9",
        "branch": "lineage-19.1",
        "name": "aarch64",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "enableCcache": false,
    "params": {
      "ARCH": "arm64",
      "CC": "aarch64",
      "externalCommands": {
        "CROSS_COMPILE": "/aarch64-linux-android-4.9/bin/aarch64-linux-android-",
        "ANDROID_MAJOR_VERSION": "o"
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": true
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
